crab -create -cfg HeavyFlavor_highPt_crab_pp.cfg
crab -create -cfg HeavyFlavor_minbias1_crab_pPb.cfg
crab -create -cfg HeavyFlavor_minbias2_crab_pPb.cfg
crab -create -cfg HeavyFlavor_minbias_crab_pp.cfg

crab -submit 1-500  -c crab_HeavyFlavor_highPt_crab_pp
crab -submit 500-1000  -c crab_HeavyFlavor_highPt_crab_pp
crab -submit 1000-1500  -c crab_HeavyFlavor_highPt_crab_pp
crab -submit 1-500 -c crab_HeavyFlavor_minbias1_crab_pPb
crab -submit 1-500 -c crab_HeavyFlavor_minbias2_crab_pPb
crab -submit 1-500 -c crab_HeavyFlavor_minbias_crab_pp
crab -submit 500-1000 -c crab_HeavyFlavor_minbias_crab_pp
